def computegrade(score):
  if score>0.9:
   return('A')
  if 0.9>score>0.8:
   print('B')
  if 0.8>score>0.7:
   print('C')
  if 0.7>score>0.6:
   print('D')
  if score<=0.6:
   print('F')
  else:
    print('bad score')
